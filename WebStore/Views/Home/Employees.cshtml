@model IEnumerable<WebStore.Models.Employee>

<h3>Сотрудники</h3>

    <table>
        <thead>
            <th>Id</th>
            <th>Имя</th>
            <th>Фамилия</th>
            <th>Отчество</th>
            <th>Возраст</th>
        </thead>
        <tbody>
            @foreach(var employee in Model)
            {
                <tr>
                    <td>@employee.Id</td>
                    <td>@employee.Name</td>
                    <td>
                        <a href = "/Home/EmployeeDetails/@employee.Id">@employee.LastName</a>
                    </td>
                    <td>@employee.Patronymic</td>
                    <td>@employee.Age</td>
                </tr>
            }
        </tbody>
    </table>

<a href = "/">На главную</a>